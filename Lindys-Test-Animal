<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Empty Canvas</title>
<script>

function drawAnimal() {
    var canvas = document.getElementById('canvas');
	var ctx = canvas.getContext('2d');
	
	// Draw grid to make it easier to position lines
	for (i = 0; i < Math.floor(canvas.height / 10); i++) {
		for (j = 0; j < Math.floor(canvas.width / 10); j++) {
			ctx.fillRect(j*10,i*10,1,1);
		}
	}
	

	// Start your JavaScript Here
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	// END your JavaScript Here
}
</script>
</head>

<body onload="drawAnimal()">
<canvas id="canvas" width="430" height="330"></canvas>

</body>

</html>
